<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>

<h1 class="text-center">Partidos</h1>

<div class="table table-warning table-striped">
  <table>
      <thead>
          <tr>
              <th>Id Pis</th>
              <th>Nombre Pais</th>
          </tr>

      </thead>

      <tbody></tbody>


  </table>

</div>

<script type="text/javascript">

    $(document).ready(function () {
        Pais();

    });

    function Pais() {
        $.ajax({
            type: 'GET',
            url: 'http://192.168.0.124/api/partido',
            datatype: 'json',
            success: function (data) {
                $.each(data, function (i, partido) {
                    var filas =
                        "<tr>"
                        + "<td>" + partido.idPartido + "</td>"
                        + "<td>" + alumno.inscripcionDate + "</td>"
                        + "<td>" + alumno.certificationDate + "</td>"
                        + "<td>" + alumno.advance + "</td>"
                        + "<td>" + alumno.scoreCourse + "</td>"
                        + "<td>" + alumno.folioCertificate + "</td>"
                        + "<td>" + alumno.course.name + "</td>"
                        + "<td>" + "<img src=" + alumno.course.imageUrl + "  width='100' height='100' /img>" + "</td>"
                        + "</tr>"
                    $("#tblAlumnos tbody").append(filas);
                    console.log(filas);
                });
            },
            error: function (data) {
                alert: ("Error");
            }

        });
    };

</script>
